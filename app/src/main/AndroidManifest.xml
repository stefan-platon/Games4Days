<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.games4days">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Games4Days"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StorePageActivity" />
        <activity android:name=".PrefActivity" />
        <activity
            android:name=".SensorsActivity"
            android:label="@string/title_activity_sensors"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LocationActivity"></activity>
    </application>

</manifest>